{
  "globals": {
    "Promise": true
  },
  "env": {
    "node": true,
    "jasmine": true
  },
  "ecmaFeatures": {
    "globalReturn": false
  },
  "rules": {
    "new-cap": false,
    "one-var": false,
    "no-underscore-dangle": false,
    "no-use-before-define": false,
    "quotes": [4, "single"],
    "comma-dangle": "always"
  }
}
